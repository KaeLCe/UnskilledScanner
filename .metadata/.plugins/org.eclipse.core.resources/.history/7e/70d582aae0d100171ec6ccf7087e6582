package portscanner.ui;

import java.util.Scanner;

public class Interface {

	private Scanner reader;
	
	Interface(){
		this.reader = new Scanner(System.in);
	}
	
	public void start() {
		System.out.println("**************************");
		System.out.println("***  Scanner de ports  ***");
		System.out.println("**************************");
	}
	
}
